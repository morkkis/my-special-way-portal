{
  "name": "msw-client",
  "version": "0.0.21",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod",
    "bump": "yarn version --new-version patch",
    "ci-e2e": "ng e2e --config protractor.ci.conf.js ",
    "ci-test": "jest --coverage && npx codecov",
    "docker": "bash ./scripts/docker",
    "docker:push": "bash ./scripts/docker-push",
    "e2e": "ng e2e",
    "git:push": "bash ./scripts/git-push",
    "lint": "ng lint",
    "ng": "ng",
    "test": "jest --coverage",
    "tslint": "tslint -p tsconfig.json -c tslint.json '**/*.ts' -e 'node_modules/**' -e 'src/polyfills.ts'",
    "start": "ng serve",
    "mock-graphql": "npx json-graphql-server ./src/mocks/db.js"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.2",
    "@angular/animations": "5.2.9",
    "@angular/cdk": "5.2.4",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/material": "5.2.4",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/platform-server": "5.2.9",
    "@angular/router": "5.2.9",
    "ajv": "6.4.0",
    "apollo-angular": "^1.1.0",
    "apollo-angular-link-http": "^1.1.0",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "apollo-client-rxjs": "^0.6.0-rc.2",
    "apollo-link-context": "^1.0.8",
    "arrive": "2.4.1",
    "bootstrap": "4.1.0",
    "bootstrap-material-design": "4.1.1",
    "bootstrap-notify": "3.1.3",
    "chartist": "0.11.0",
    "classlist.js": "1.1.20150312",
    "core-js": "2.4.1",
    "googleapis": "28.1.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "hammerjs": "2.0.8",
    "jquery": "3.2.1",
    "moment": "2.18.1",
    "perfect-scrollbar": "1.1.0",
    "popper.js": "1.14.3",
    "rxjs": "5.5.10",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.9",
    "@angular/language-service": "5.2.9",
    "@types/bootstrap": "3.3.32",
    "@types/chartist": "0.9.34",
    "@types/googlemaps": "^3.30.8",
    "@types/jest": "^23.0.0",
    "@types/jquery": "1.10.31",
    "@types/node": "6.0.73",
    "codecov": "^3.0.2",
    "codelyzer": "4.2.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "^23.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-preset-angular": "^5.2.2",
    "jest-zone-patch": "^0.0.8",
    "json-graphql-server": "^2.1.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.1",
    "protractor": "5.3.1",
    "shallow-render": "^6.0.3",
    "ts-node": "5.0.1",
    "tslint": "5.10.0",
    "typescript": "2.4.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "roots": [
      "src/app"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.module.ts",
      "!**/app.routing.ts",
      "!**/*.d.ts",
      "!src/setupJest.ts",
      "!src/app/models/*",
      "!**/*.tmp.ts",
      "!**/*.dialog.ts",
      "!**/login.component.ts",
      "!**/jwt.interceptor.ts",
      "!**/dashboard.component.ts"
    ]
  }
}
